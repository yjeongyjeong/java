package ex11;

import java.util.ArrayList;
import java.util.Queue;

public class LinkedListQueue {
	public static void main(String[] args) {
//		Queue<String> queue = new LinkedList<String>();
		Queue<String> queue = new ArrayList<String>();

		queue.add("Toy");
		queue.offer("Box");
		queue.offer("Robot");
		queue.offer("Toy");

		System.out.println(queue.poll()); //-> 맨첨에 들어온 toy 하나를 빼버림
		System.out.println(queue.poll()); //-> 다음에 들어온 box 하나를 빼버림
		System.out.println("---------------");
		
		for (String s : queue)
			System.out.println(s); //First Input First Output 들어온순서대로 나감

		
		System.out.println("---------------");
		for (String s : queue)
			System.out.println(s); //First Input First Output 들어온순서대로 나감
		System.out.println("---------------!");
		System.out.println(queue.peek()); //다음에 빠져나올 값?
		System.out.println("---------------");
		for (String s : queue)
			System.out.println(s); 
	}
}
