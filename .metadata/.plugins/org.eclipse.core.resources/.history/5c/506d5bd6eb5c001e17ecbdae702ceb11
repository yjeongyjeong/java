package ex07;

import java.util.HashSet;
import java.util.Iterator;
import java.util.Set;

// 정수나 실수 + 문자열 => 결과는 문자열

class Num{
	private int num;
	public Num(int num) { this.num = num; }
	@Override
	public String toString() {	return String.valueOf(num) ;}
	//정수를 문자열로..
	
	@Override
	public int hashCode() {
		System.out.println("hash => "); //add할때 전부 시행됨
		return num % 3;
	}
	@Override
	public boolean equals(Object obj) {
//		if(num == ((Num)obj).num)
//			return true;
//		else
//			return false;
		System.out.println("equals => "); //hash에서 같은값이 있는 경우 한번 비교됨
		return num == ((Num)obj).num ? true : false;
	}
}

public class SetCollection3 {
	public static void main(String[] args) {
		Set<Num> set = new HashSet<>();
		
		set.add(new Num(9)); //ex. 주소값 0x10
		set.add(new Num(12));
		set.add(new Num(7));
		set.add(new Num(9)); //ex. 주소값 0x20
		
		Iterator<Num> iter = set.iterator();
		Num str; 
		while(iter.hasNext()) {
			System.out.println(iter.next()); //순서대로 출력되지 않음
//			근데 값이 다나옴 9999가 중복되는데 그냥 잘 들어가서 저장됨!!!
//			이를 방지하려면 hashcode, equals를 재정의해주기
		}
	}
}
