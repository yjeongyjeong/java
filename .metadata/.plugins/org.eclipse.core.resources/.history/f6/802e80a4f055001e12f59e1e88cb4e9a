package ex02;

public class Exam05 {
	public static void main(String[] args) {
		int A = 100;
		int B = 500;
		int C = 1000;
		
		if( A>B) {
			if(A>C) {
				System.out.println("Max : " + A);
			} else { 
				System.out.println("Max : " + C);
			}
		} else {
			System.out.println("Max : " + B);
		}
	}
}
