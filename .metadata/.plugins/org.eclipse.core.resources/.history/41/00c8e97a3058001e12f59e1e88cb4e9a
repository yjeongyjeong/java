package ex04;

public class TVUser {
	public static void main(String[] args) {
		TV tv = new LgTVSub(); //추상클래스는 객체를 구현할 수 없음
		tv.powerOn();
		tv.volumeUp();
		tv.volumeDown();
		tv.powerOff();

		TV tv2 = new SamsungTV();
		tv2.powerOn();
		tv2.volumeUp();
		tv2.volumeDown();
		tv2.powerOff();
	}

}
