package ex03_refactoring;

public class LgTV {

	public void turnOn() {
		System.out.println("LgTV 전원을 키다.");
	}
	
	public void turnOff() {
		System.out.println("LgTV 전원을 끄다.");
	}
	
	public void soundUp() {
		System.out.println("LgTV 소리를 크게하다.");
	}
	
	public void soundDown() {
		System.out.println("LgTV 소리를 작게하다.");
	}
	
}
