package ex05;

public class Main {
	public static void main(String[] args) {
		
		A a = new A();
		a.funcA();
		
		//extend A
		B b = new B();
		b.funcB();
		b.funcA();
		
		//extend B
		C c = new C();
		c.funcA();
		c.funcB();
		c.funcC();
		System.out.println("=================");
		
		//상위타입에서 하위타입으로 만들어주면 A에 있는 애들에게만 접근가능 B에 접근하려면 B에 오버라이딩된게 있어야 함
		A aa = new B();
		aa.funcA(); // A 클래스 내의 A메서드 출력
	}
}
