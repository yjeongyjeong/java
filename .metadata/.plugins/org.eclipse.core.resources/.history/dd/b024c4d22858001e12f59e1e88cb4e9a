package ex03;

public class LgTV {

	public void powerOn() {
		System.out.println("LgTV 전원을 키다.");
	}
	
	public void powerOff() {
		System.out.println("LgTV 전원을 끄다.");
	}
	
	public void volumeUp() {
		System.out.println("LgTV 소리를 키우다.");
	}
	
	public void volumeDown() {
		System.out.println("LgTV 소리를 줄이다.");
	}
	
}
