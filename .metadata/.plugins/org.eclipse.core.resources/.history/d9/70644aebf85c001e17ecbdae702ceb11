package ex09;

import java.util.HashSet;
import java.util.Iterator;
import java.util.Set;

/*
 * 이름과 나이가 같으면 동일 인스턴스로 판단되도록 해시코드와 이퀄스를 재정의하기
 * */
class Person {
	private String name;
	private int age;

	public Person(String name, int age) {
	}

	@Override
	public int hashCode() {
		return super.hashCode();
	}

	@Override
	public boolean equals(Object obj) {
		return super.equals(obj);
	}

	@Override
	public String toString() {
		return name + "(" + age + ")";
	}
}

public class SetCollectionExam01 {
	public static void main(String[] args) {
		Set<Person> p = new HashSet<Person>();

		p.add(new Person("홍길동", 20));
		p.add(new Person("아이스", 40));
		p.add(new Person("홍길동", 20));
		p.add(new Person("브레이커스", 37));

//		Iterator<Person> iter = p.iterator();
//		while (iter.hasNext()) {
//			Person print = iter.next();
//			System.out.println(print);
//		}
		
		for(Person print : p)
			System.out.println(print);
	}
}
