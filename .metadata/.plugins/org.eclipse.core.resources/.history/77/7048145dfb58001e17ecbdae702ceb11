package ex02;

import java.util.Scanner;

public class ExceptionEx01 {
	public static void main(String[] args) {
		System.out.println("예외처리1");
		System.out.println("예외처리1");
		System.out.println("예외처리1");
		System.out.println("예외처리1");
		
		Scanner sc = new Scanner(System.in);
		System.out.println("예외처리1");
		System.out.println("예외처리1");
		System.out.println("예외처리1");
		System.out.println("예외처리1");
		System.out.println("프로그램 종료!!");
	}
}
