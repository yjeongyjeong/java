package ex02_1;

class Apple {
	public String toString() {
		return "I an an apple";
	}
}

class Orange {
	public String toString() {
		return "I am an orange";
	}
}
//중복된 내용을 제거
class Box { // 담는 박스
	private Object obj;

	void setApple(Object obj) {
		this.obj = obj;
	}

	Object getApple() {
		return obj;
	}
}

