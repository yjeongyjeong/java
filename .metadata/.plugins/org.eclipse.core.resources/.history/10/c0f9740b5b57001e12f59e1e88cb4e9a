package ex03;

public class TV {

	String color;
	boolean power; //기본값 false
	int channel;
	//생성자의 역할 : 멤버변수 초기화에 이용될 수 있다,,
	TV(){
		System.out.println("기본생성자");
	}
	
	TV(int c) {
		channel = c;
	}

	TV(String string, int i) {
		System.out.println("매개변수 두 개");
	}

	void power() {
		power = !power;
	}
	
	void channelUp() {
		channel++;
	}
	
	void channelDown() {
		channel--;
	}
}
