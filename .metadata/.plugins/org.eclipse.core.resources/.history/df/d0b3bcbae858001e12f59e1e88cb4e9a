package ex01;

import java.util.List;

public class Ex01 {
public static void main(String[] args) {

	Person p1 = new Person("나나나", 20);
	Person p2 = new Person("나나나", 20);
	System.out.println(p1.equals(p2));
}
}
class Person{
	String name;
	int age;
	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
}