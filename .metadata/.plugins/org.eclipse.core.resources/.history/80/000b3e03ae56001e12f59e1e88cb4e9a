package ex02;

public class Exam07 {
	public static void main(String[] args) {
		int[] numA = new int[10];

		for (int i = 0; i < 10; i++) {
			numA[i] = i + 1;
		}
		System.out.println("원 데이터 : ");
		for (int i = 0; i < numA.length; i++)
			System.out.print(numA[i] + " ");
		
		for (int i = 0; i < 100; i++) {
			int n = (int)(Math.random()*100); //0~99
			int tmp = numA[0];
			numA[0] = numA[n];
			numA[n] = tmp;
		}
		System.out.println("썩은 데이터 : ");
		
		

	}
}
