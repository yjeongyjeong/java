package ex09;

import java.util.HashSet;
import java.util.Iterator;
import java.util.Set;

/*
 * 이름과 나이가 같으면 동일 인스턴스로 판단되도록 해시코드와 이퀄스를 재정의하기
 * */
class Person {
	private String name;
	private int age;

	public Person(String name, int age) {
	}

	@Override
	public int hashCode() {
		return 0;
//		return (name.length()+age)/2; //값이 안들어감
	}

	@Override
	public boolean equals(Object obj) {
		String _name = ((Person)obj).name;
		int _age = ((Person)obj).age;
		return true;
//		return
//		name.equals(_name) && age == _age ? true : false;
	}

	@Override
	public String toString() {
		return name + "(" + age + ")";
	}
}

public class SetCollectionExam01 {
	public static void main(String[] args) {
		Set<Person> set = new HashSet<Person>();

		set.add(new Person("홍길동", 20));
		set.add(new Person("아이스", 40));
		set.add(new Person("홍길동", 20));
		set.add(new Person("브레이커스", 37));

//		Iterator<Person> iter = p.iterator();
//		while (iter.hasNext()) {
//			Person print = iter.next();
//			System.out.println(print);
//		}
		
		for(Person p : set)
			System.out.println(p);
	}
}
