package ex02_1;

class Apple {
	public String toString() {
		return "I an an apple";
	}
}

class Orange {
	public String toString() {
		return "I am an orange";
	}
}
//중복된 내용을 제거
class Box { // 담는 박스
	private Object obj;

	void setObj(Object obj) {
		this.obj = obj;
	}

	Object getObj() {
		return obj;
	}
}

public class Generices1 {
	public static void main(String[] args) {
		Box aBox = new Box(); //사과
		Box oBox = new Box(); //오렌지
		
		aBox.
	}
}
