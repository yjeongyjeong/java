package ex03_refactoring;

public class TV {
//이걸 인터페이스로 빼도 됨 대신 인터페이스로 뺀다면 powerOn();이렇게만 있고 내용은 구현된곳에서 알아서 구현..
	public void powerOn() {
		System.out.println("TV 전원을 키다.");
	}
	
	public void powerOff() {
		System.out.println("TV 전원을 끄다.");
	}
	
	public void volumeUp() {
		System.out.println("TV 소리를 크게하다.");
	}
	
	public void volumeDown() {
		System.out.println("TV 소리를 작게하다.");
	}
	
}
