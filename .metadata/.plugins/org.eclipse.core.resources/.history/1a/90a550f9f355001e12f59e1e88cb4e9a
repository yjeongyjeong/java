package ex02;

public class Exam06 {
	public static void main(String[] args) {
		int num = 3;
		
		switch(num) {
		case 1: System.out.println("1");
		case 2: System.out.println("2");
		case 3: System.out.println("3");
		case 4: System.out.println("4");
		default : System.out.println("default");
		}
	}

}
