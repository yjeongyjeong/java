public class AbstractMenu implements Menu{

	protected String menuText; //기본문구
	protected Menu prevMenu; //이전 메뉴
	protected static final Scanner sc = new Scanner(System.in);
	
	
	@Override
	public void print() {
		System.out.println();
	}

	@Override
	public Menu next() {
		return null;
	}

}
