package ex02;

public class TV {

	String color;
	boolean power;
	int channel;
	//접근제한자는 있을 수 있지만 반환타입은 없음
	TV(){
		System.out.println("기본생성자");
	}
	
	TV(int i) {
		System.out.println("매개변수 하나");
	}

	TV(String string, int i) {
		System.out.println("매개변수 두 개");
	}

	void power() {
		power = !power;
	}
	
	void channelUp() {
		channel++;
	}
	
	void channelDown() {
		channel--;
	}
}
