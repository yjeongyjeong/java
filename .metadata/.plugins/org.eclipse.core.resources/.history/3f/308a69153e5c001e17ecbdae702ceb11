package ex02_4;

class Box<T> {
	private T ob; // 필드

	public T get() { // getter 메서드
		return ob;
	}
	public void set(T ob) { // setter 메서드
		this.ob = ob;
	}
	@Override
	public String toString() {
		return ob.toString();
	}
}

class Toy {
	public String toString() { return "I am a Toy!"; }
}

class BoxHandler{ 
	public static void outBox(Box<? extends Toy> box) { //꺼내기
		Toy t = box.get();
		System.out.println(t);
		box.set(new Toy());
	}
	public static void inBox(Box<Toy> box, Toy n) { //저장하기
		box.set(n);
		Toy n2 = box.get();
	}
}

public class WildCardUnBox {
	public static void main(String[] args) {
		Box<Toy> box = new Box(); //Toy타입의 객체 box를 생성
		BoxHandler.inBox(box, new Toy());
		
		BoxHandler.outBox(box);
	}
}
